{
	"name": "directus-extension-image-hotspot",
	"description": " A interface to pin hotspots to image",
	"icon": "extension",
	"version": "1.1.1",
	"author": {
		"email": "contact@codihaus.com",
		"name": "Codihaus"
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-interface"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"release": "pnpm run build && changelogen --release && npm publish && git push --follow-tags",
		"release:t": "pnpm run build && changelogen --release && npm publish"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.0.4",
		"typescript": "^5.8.3",
		"vue": "^3.5.13"
	},
	"dependencies": {
		"@vueuse/core": "^13.1.0",
		"changelogen": "^0.6.1",
		"lodash-es": "^4.17.21",
		"vue-i18n": "^11.1.3"
	}
}
